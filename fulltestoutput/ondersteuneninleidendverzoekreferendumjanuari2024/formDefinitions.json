                {
                    "label": "continue",
                    "calculateValue": "    value = false;\n   \n   if(data.Woonplaats) { value = true;}\n    if (data.inlogmiddel) {\n        if (data.inlogmiddel == 'digid' || data.inlogmiddel == 'irma') {\n\n            if (data.woonplaats == 'Nijmegen' || data.woonplaats == 'Lent') {\n                var geboorteDatumSplitted = data.geboortedatum.split('-');\n                var geboortedatumJaar = geboorteDatumSplitted[2];\n                var geboortedatumMaand = geboorteDatumSplitted[1];\n                var geboortedatumDag = geboorteDatumSplitted[0];\n                // Add day and month if they are missing, happens in rare cases\n                if (!geboortedatumDag) {\n                    geboortedatumDag = 1;\n                }\n                if (!geboortedatumMaand) {\n                    geboortedatumMaand = 5;\n                }\n\n\n                //  Max date of birth to be able to participate in the referendum\n                var maxGeboortejaar = 2006\n                var maxGeboortemaand = 1 // January\n                var maxGeboortedag = 24\n\n                if (geboortedatumJaar < maxGeboortejaar) {\n                    value = true;\n                }\n                else if (geboortedatumJaar == maxGeboortejaar) {\n                    if (geboortedatumMaand < maxGeboortemaand) {\n                        value = true;\n                    }\n                    else if (geboortedatumMaand == maxGeboortemaand && geboortedatumDag <= maxGeboortedag) {\n                        value = true;\n                    }\n                }\n            }\n        }\n    }",
